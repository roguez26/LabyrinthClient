      <xs:element minOccurs="0" name="Username" nillable="true" type="xs:string" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="TransferUser" nillable="true" type="tns:TransferUser" />
  <xs:complexType name="TransferStats">
